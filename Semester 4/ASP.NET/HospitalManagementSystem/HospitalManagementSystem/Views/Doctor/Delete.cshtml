@model Doctor

<h1>Are you sure you want to delete this doctor?</h1>

<form asp-action="Delete"  asp-controller="Doctor" method="post">
	<input asp-for="Id" hidden />
	
	<label asp-for="Name"></label>
	<input asp-for="Name" type="text" placeholder="Name" disabled />
	<br />
	<br />
	<label asp-for="Office"></label>
	<input asp-for="Office" type="text" placeholder="Office" disabled />
	<br />
	<br />
	<label asp-for="Practice"></label>
	<input asp-for="Practice" type="text" placeholder="Practice" disabled />
	<br />
	<br />
	<button>Delete</button>
	<a asp-action="Index" asp-controller="Doctor">Back to doctors</a>
</form>